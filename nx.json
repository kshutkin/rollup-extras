{
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "lint",
          "semantic-release"
        ],
        "accessToken": "MzFhNzYxYjUtYWRjYi00MTIyLWExNWItZjNjMmFlNTYzNzg3fHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDefaults": {
    "build": {
      "inputs": [
        { "runtime": "node -v" }
      ],
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "test": {
      "dependsOn": [
        "^test", "build"
      ],
      "outputs": [
        "{projectRoot}/coverage"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "semantic-release": {
      "dependsOn": [
        "^semantic-release"
      ]
    }
  },
  "defaultBase": "main"
}
